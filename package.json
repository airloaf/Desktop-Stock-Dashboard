{
  "name": "desktop-stock-dashboard",
  "version": "1.0.0",
  "description": "Stock Dashboard for desktops",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Desktop Stock Dashboard\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64  --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airloaf/Desktop-Stock-Dashboard.git"
  },
  "author": "Vikram Singh vikzilla18@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airloaf/Desktop-Stock-Dashboard/issues"
  },
  "homepage": "https://github.com/airloaf/Desktop-Stock-Dashboard#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.1",
    "electron": "^1.7.10",
    "electron-settings": "^3.1.4",
    "fs-jetpack": "^1.2.0",
    "materialize-css": "^0.100.2"
  },
  "devDependencies": {
    "electron-packager": "^10.1.0"
  }
}
